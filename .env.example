# Laminar Configuration
# Copy this file to .env and fill in your values

# Anthropic/Claude API Configuration
LAMINAR_ANTHROPIC_API_KEY=your-api-key-here
LAMINAR_CLAUDE_MODEL=claude-sonnet-4-20250514
LAMINAR_CLAUDE_MAX_TOKENS=8192
LAMINAR_CLAUDE_TEMPERATURE=0.0

# Excel Processing Mode
# Options: "image" (visual only), "csv" (data only), "both" (recommended)
LAMINAR_EXCEL_MODE=both

# Docker Configuration
LAMINAR_DOCKER_IMAGE=xls2png-converter

# Path Configuration
LAMINAR_OUTPUT_DIR=output
LAMINAR_SAMPLE_JSON_PATH=sample.json
LAMINAR_SAMPLE_IMAGE_PATH=sample.png

# Logging Configuration
LAMINAR_LOG_LEVEL=INFO
# LAMINAR_LOG_FILE=laminar.log
