# Laminar Configuration
# Copy this file to .env and fill in your values

# OpenAI API Configuration
LAMINAR_OPENAI_API_KEY=your-api-key-here
LAMINAR_OPENAI_MODEL=gpt-4o
LAMINAR_OPENAI_MAX_TOKENS=4096
LAMINAR_OPENAI_TEMPERATURE=0.0

# Docker Configuration
LAMINAR_DOCKER_IMAGE=xls2png-converter

# Path Configuration
LAMINAR_OUTPUT_DIR=output
LAMINAR_SAMPLE_JSON_PATH=sample.json
LAMINAR_SAMPLE_IMAGE_PATH=sample.png

# Logging Configuration
LAMINAR_LOG_LEVEL=INFO
# LAMINAR_LOG_FILE=laminar.log
